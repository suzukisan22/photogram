<div class="col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1">
  <ul class="nav nav-tabs">
    <li class="<%= isActiveTab('user') %>">
      <%= link_to 'Users', search_path(for: 'user', q: params[:q]) %>
    </li>
    <li class="<%= isActiveTab('picture') %>">
      <%= link_to 'Pictures', search_path(for: 'picture', q: params[:q]) %>
    </li>
  </ul>
  <% @objects.each do |object| %>
    <% if params[:for] == 'user' %>
      <%= object.username %>
    <% else %>
      <%= object.comment %>
    <% end %>
  <% end %>
</div>
