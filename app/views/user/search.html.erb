<div class="col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1">
  <ul class="nav nav-tabs">
    <li class="<%= isActiveTab('user') %>">
      <%= link_to 'Users', search_path(by: 'user', q: params[:q]) %>
    </li>
    <li class="<%= isActiveTab('picture') %>">
      <%= link_to 'Pictures', search_path(by: 'picture', q: params[:q]) %>
    </li>
    <li class="<%= isActiveTab('tag') %>">
      <%= link_to 'Tags', search_path(by: 'tag', q: params[:q]) %>
    </li>
  </ul>
  <% @objects.each do |object| %>
    <div class="col-sm-4 object-area">
      <%= render partial: 'result', locals: { object: object } %>
    </div>
  <% end %>
</div>
